{
  "name": "int-to-roman",
  "version": "0.0.0",
  "description": "A simple number to roman number converter",
  "keywords": [],
  "main": "dist/to-roman.js",
  "module": "dist/to-roman.mjs",
  "browser": "dist/to-roman.umd.js",
  "types": "types/index.d.ts",
  "bundlib": {
    "min": "browser",
    "equals": true,
    "name": "toRoman"
  },
  "scripts": {
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "test": "jest --color -w 3",
    "test:watch": "npm test -- --watch-all",
    "clean": "rimraf dist coverage types node_modules/.cache/jest node_modules/.cache/bundlib",
    "lint": "eslint src/** __test__/**",
    "prerelease": "npm run clean && npm test && npm run build",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "bundlib": "^0.14.2",
    "chokidar": "^3.3.1",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "rimraf": "^3.0.2",
    "standard-version": "^7.1.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.5"
  },
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/int-to-roman.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/int-to-roman/issues"
  },
  "homepage": "https://github.com/manferlo81/int-to-roman#readme",
  "license": "MIT"
}

{
  "name": "int-to-roman",
  "version": "0.0.0",
  "description": "A simple number to roman number converter",
  "keywords": [],
  "main": "dist/to-roman.js",
  "module": "dist/to-roman.mjs",
  "browser": "dist/to-roman.umd.js",
  "types": "types/index.d.ts",
  "bundlib": {
    "name": "toRoman",
    "min": "browser"
  },
  "scripts": {
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "test": "jest --color -w 3",
    "test:watch": "npm test -- --watch-all",
    "clean": "rimraf dist coverage types node_modules/.cache/jest node_modules/.cache/bundlib",
    "lint": "eslint src/** __test__/**",
    "prerelease": "npm run clean && npm run lint && npm test && npm run build",
    "release": "standard-version"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "bundlib": "^0.16.3",
    "chokidar": "^3.4.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-export-equals": "^0.3.6",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "standard-version": "^8.0.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/int-to-roman.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/int-to-roman/issues"
  },

{
  "name": "int-to-roman",
  "version": "0.0.0",
  "description": "A simple number to roman number converter",
  "keywords": [
    "convert",
    "converter",
    "int",
    "integer",
    "number",
    "roman",
    "roman-number"
  ],
  "main": "dist/to-roman.js",
  "module": "dist/to-roman.mjs",
  "browser": "dist/to-roman.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "test": "jest --color -w 3",
    "test:watch": "npm test -- --watch-all",
    "clean": "rimraf dist coverage node_modules/.cache/jest node_modules/.cache/bundlib",
    "lint": "eslint src/**/*.ts __test__/**/*.ts",
    "prerelease": "npm run clean && npm run lint && npm test && npm run build",
    "release": "standard-version"
  },
  "engines": {
    "node": "6"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.24.0",
    "bundlib": "^0.18.4",
    "chokidar": "^3.5.1",
    "eslint": "^7.27.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/int-to-roman.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/int-to-roman/issues"
  },
  "homepage": "https://github.com/manferlo81/int-to-roman#readme",
  "license": "MIT"
}
